<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.news">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.android.news.PunjabiActivity"></activity>
        <activity android:name="com.android.news.ContainActivity" />
        <activity android:name="com.android.news.LoadActivity" />
        <activity android:name="com.android.news.EnglishActivity" />
        <activity android:name="com.android.news.HindiDashboardActivity" />
        <activity android:name="com.android.news.CategoryActivity" />
        <activity android:name="com.android.news.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>